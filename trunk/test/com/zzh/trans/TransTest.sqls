.company.drop:
DROP TABLE IF EXISTS `company`;
#-------------------------------------------------------------------------
.company.create:
CREATE TABLE `company` (
  `id` int(4) NOT NULL auto_increment,
  `name` varchar(32) NOT NULL,
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
#-------------------------------------------------------------------------
.master.drop:
DROP TABLE IF EXISTS `master`;
#-------------------------------------------------------------------------
.master.create:
CREATE TABLE `master` (
  `id` int(4) NOT NULL auto_increment,
  `name` varchar(32) NOT NULL,
  `comId` int(4),
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
#-------------------------------------------------------------------------
.cat.drop:
DROP TABLE IF EXISTS `cat`;
#-------------------------------------------------------------------------
.cat.create:
CREATE TABLE `cat` (
  `id` int(4) NOT NULL auto_increment,
  `name` varchar(32) NOT NULL,
  `masterId` int(4),
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
