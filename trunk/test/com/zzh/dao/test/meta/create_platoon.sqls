/*--------------------------------------------------*/
/*create.soldier*/
CREATE TABLE dao_d_soldier_${.id} (
  name  VARCHAR(50) PRIMARY KEY,
  age   INT
);
/*--------------------------------------------------*/
/*create.tank*/
CREATE TABLE dao_d_tank_${.id} (
  id  SERIAL PRIMARY KEY,
  code  VARCHAR(50) NOT NULL UNIQUE,
  weight INT,
  motorName  VARCHAR(50) 
);
/*--------------------------------------------------*/
/*create.gun*/
CREATE TABLE dao_d_gun_${.id} (
  id  SERIAL PRIMARY KEY,
  sname  VARCHAR(50) ,
  type  VARCHAR(20) 
);
/*--------------------------------------------------*/
/*create.mission*/
CREATE TABLE dao_d_mission_${.id} (
  name  VARCHAR(50) PRIMARY KEY,
  dday  TIMESTAMP 
);
/*--------------------------------------------------*/
/*create.soldier.tank*/
CREATE TABLE dao_d_m_soldier_tank_${.id} (
  sname VARCHAR(50),
  tid INT
);