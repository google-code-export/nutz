.abc.drop:
DROP TABLE IF EXISTS `t_abc`;
#-------------------------------------------------------------------------
.abc.create:
CREATE TABLE `t_abc` (
  `id` int(11) NOT NULL auto_increment,
  `name` varchar(32) NOT NULL,
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
