/*.host.drop*/
DROP TABLE IF EXISTS mo_host;
/*------------------------------------------------------------------------*/
/*.host.create*/
CREATE TABLE mo_host (
  id SERIAL PRIMARY KEY,
  name VARCHAR(32) NOT NULL UNIQUE,
  tid INT NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.other.drop*/
DROP TABLE IF EXISTS mo_other;
/*------------------------------------------------------------------------*/
/*.other.create*/
CREATE TABLE mo_other (
  hid BIGINT,
  text VARCHAR(8) NOT NULL UNIQUE
)${.engine};
/*------------------------------------------------------------------------*/
/*.type.drop*/
DROP TABLE IF EXISTS mo_host_type;
/*------------------------------------------------------------------------*/
/*.type.create*/
CREATE TABLE mo_host_type (
  id INT UNIQUE,
  text VARCHAR(6) NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.adr.drop*/
DROP TABLE IF EXISTS mo_address;
/*------------------------------------------------------------------------*/
/*.adr.create*/
CREATE TABLE mo_address (
  ip VARCHAR(16) NOT NULL PRIMARY KEY,
  cmt VARCHAR(20)
)${.engine};
/*------------------------------------------------------------------------*/
/*.port.drop*/
DROP TABLE IF EXISTS mo_port;
/*------------------------------------------------------------------------*/
/*.port.create*/
CREATE TABLE mo_port (
  id SERIAL PRIMARY KEY,
  value INT NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.host_adr.drop*/
DROP TABLE IF EXISTS mo_host_address;
/*------------------------------------------------------------------------*/
/*.host_adr.create*/
CREATE TABLE mo_host_address (
  host_name VARCHAR(32) NOT NULL,
  address_ip VARCHAR(16) NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.host_port.drop*/
DROP TABLE IF EXISTS mo_host_port;
/*------------------------------------------------------------------------*/
/*.host_port.create*/
CREATE TABLE mo_host_port (
  host_id BIGINT NOT NULL,
  port_id BIGINT NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.host_main_adr.drop*/
DROP TABLE IF EXISTS mo_host_main_address;
/*------------------------------------------------------------------------*/
/*.host_main_adr.create*/
CREATE TABLE mo_host_main_address (
  host_name VARCHAR(32) NOT NULL,
  address_ip VARCHAR(16) NOT NULL
)${.engine};
/*------------------------------------------------------------------------*/
/*.host_main_port.drop*/
DROP TABLE IF EXISTS mo_host_main_port;
/*------------------------------------------------------------------------*/
/*.host_main_port.create*/
CREATE TABLE mo_host_main_port (
  host_id BIGINT NOT NULL,
  port_id BIGINT NOT NULL
)${.engine};
