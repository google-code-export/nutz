nut_object {
  id INT +PK,
  name VARCHAR(32) !UNIQUE,
  parent VARCHAR(32),
  type VARCHAR(256),
  singleton BOOLEAN DEFAULT '0',
  cmt TEXT
}

nut_lifecycle {
  id INT +PK,
  born VARCHAR(100),
  depose VARCHAR(100),
  fetch VARCHAR(100)
}

nut_field {
  oid INT,
  name VARCHAR(32) !,
  vid INT
}

nut_value {
  id INT +PK,
  type VARCHAR(32),
  value TEXT 
}

nut_obj_args {
  oid INT,
  vid INT
}